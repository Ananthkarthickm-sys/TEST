      "parameters": {
        "jsCode": "const data = items[0].json;\nconst Id = data.id;\nconst templates = data.templates || {};\n\nconst taskSequence = [\n  \"resignation_proof_received\",\n  \"hr_acceptance_received\",\n  \"relocation_required\",\n  \"resignation_reminder_sent\",\n  \"hr_acceptance_reminder_sent\"\n];\n\n// Check if we have a previously completed task to mark\nconst previous_completed_task = data.previous_task || null;\nif (previous_completed_task && taskSequence.includes(previous_completed_task)) {\n  templates[previous_completed_task] = true;\n}\n\n// Find the next incomplete task\nlet next_task = null;\nfor (const task of taskSequence) {\n  if (templates[task] !== true) {\n    next_task = task;\n    break;\n  }\n}\n\nconst incompleteCount = taskSequence.reduce((count, task) => {\n  return count + (templates[task] !== true ? 1 : 0);\n}, 0);\n\nconst process_paused = incompleteCount === 1;\n\nreturn [{\n  json: {\n    id: Id,\n    next_task,\n    process_paused,\n    templates,\n    previous_task: next_task // This will be marked complete on next run\n  }\n}];"
      },
